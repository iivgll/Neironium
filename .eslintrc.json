{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/no-unescaped-entities": ["error", { "forbid": [">", "}"] }],
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "warn",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_",
        "ignoreRestSiblings": true
      }
    ]
  },
  "plugins": ["unused-imports"]
}
